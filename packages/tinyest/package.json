{
  "name": "tinyest",
  "private": true,
  "version": "0.1.0-alpha.2",
  "description": "A compact, fast, and embeddable JavaScript AST for transpilation.",
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "module": "./dist/*.js",
        "import": "./dist/*.js",
        "default": "./dist/*.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/TypeGPU.git"
  },
  "keywords": ["ast", "javascript"],
  "bugs": {
    "url": "https://github.com/software-mansion/TypeGPU/issues"
  },
  "homepage": "https://docs.swmansion.com/TypeGPU",
  "devDependencies": {
    "typescript": "catalog:",
    "tsup": "catalog:",
    "@typegpu/tgpu-dev-cli": "workspace:*"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
