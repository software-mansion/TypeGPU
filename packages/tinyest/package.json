{
  "name": "tinyest",
  "private": true,
  "version": "0.3.0",
  "description": "A compact, fast, and embeddable JavaScript AST for transpilation.",
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "module": "./dist/*.js",
        "import": "./dist/*.js",
        "default": "./dist/*.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/TypeGPU.git"
  },
  "keywords": ["ast", "javascript"],
  "bugs": {
    "url": "https://github.com/software-mansion/TypeGPU/issues"
  },
  "homepage": "https://docs.swmansion.com/TypeGPU",
  "devDependencies": {
    "typescript": "catalog:types",
    "tsdown": "catalog:build",
    "@typegpu/tgpu-dev-cli": "workspace:*"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
