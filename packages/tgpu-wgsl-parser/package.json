{
  "name": "tgpu-wgsl-parser",
  "private": true,
  "version": "0.0.0",
  "description": "A WGSL code parser.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./nearley-redirect-plugin": "./nearley-redirect-plugin.mjs"
  },
  "publishConfig": {
    "directory": "dist",
    "main": "./dist/index.js",
    "types": "./index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test:types": "pnpm tsc --p ./tsconfig.json --noEmit",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "keywords": ["webgpu", "wgpu", "wgsl", "parser", "ast"],
  "packageManager": "pnpm@8.15.8+sha256.691fe176eea9a8a80df20e4976f3dfb44a04841ceb885638fe2a26174f81e65e",
  "dependencies": {
    "nearley": "^2.20.1"
  },
  "devDependencies": {
    "@types/moo": "^0.5.9",
    "@types/nearley": "^2.11.5",
    "@webgpu/types": "^0.1.54",
    "moo": "^0.5.2",
    "tsup": "^8.0.2",
    "tsx": "^4.16.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.9",
    "@typegpu/tgpu-dev-cli": "workspace:*"
  }
}
