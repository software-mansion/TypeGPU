{
  "name": "tgpu-jit",
  "private": true,
  "version": "0.0.0",
  "description": "A Just-In-Time (JIT) transpiler for TypeGPU. Allows users to test out TestGPU without a build-step at the cost of memory and efficiency.",
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test:types": "pnpm tsc --p ./tsconfig.test.json --noEmit",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/typegpu.git"
  },
  "keywords": [
    "webgpu",
    "wgpu",
    "wgsl",
    "typescript",
    "compute",
    "shader",
    "shaders",
    "gpgpu"
  ],
  "bugs": {
    "url": "https://github.com/software-mansion/typegpu/issues"
  },
  "homepage": "https://docs.swmansion.com/typegpu",
  "devDependencies": {
    "typescript": "^5.7.3",
    "tsup": "^8.0.2",
    "@typegpu/tgpu-dev-cli": "workspace:*",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@8.15.8+sha256.691fe176eea9a8a80df20e4976f3dfb44a04841ceb885638fe2a26174f81e65e",
  "dependencies": {
    "acorn": "^8.12.1",
    "tinyest": "workspace:~0.1.0-alpha.2",
    "tinyest-for-wgsl": "workspace:~0.1.0-alpha.2"
  }
}
