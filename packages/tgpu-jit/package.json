{
  "name": "tgpu-jit",
  "private": true,
  "version": "0.0.0",
  "description": "A Just-In-Time (JIT) transpiler for TypeGPU. Allows users to test out TestGPU without a build-step at the cost of memory and efficiency.",
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test:types": "pnpm tsc --p ./tsconfig.test.json --noEmit",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/typegpu.git"
  },
  "keywords": [
    "webgpu",
    "wgpu",
    "wgsl",
    "typescript",
    "compute",
    "shader",
    "shaders",
    "gpgpu"
  ],
  "bugs": {
    "url": "https://github.com/software-mansion/typegpu/issues"
  },
  "homepage": "https://docs.swmansion.com/typegpu",
  "devDependencies": {
    "typescript": "catalog:",
    "tsup": "catalog:",
    "@typegpu/tgpu-dev-cli": "workspace:*",
    "vitest": "catalog:"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "dependencies": {
    "acorn": "^8.12.1",
    "tinyest": "workspace:~0.1.0-alpha.3",
    "tinyest-for-wgsl": "workspace:~0.1.0-alpha.3"
  }
}
