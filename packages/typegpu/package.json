{
  "name": "typegpu",
  "private": true,
  "version": "0.7.0",
  "description": "A thin layer between JS and WebGPU/WGSL that improves development experience and allows for faster iteration.",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./data": "./src/data/index.ts",
    "./std": "./src/std/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./data": {
        "types": "./dist/data/index.d.ts",
        "import": "./dist/data/index.js"
      },
      "./std": {
        "types": "./dist/std/index.d.ts",
        "import": "./dist/std/index.js"
      },
      "./package.json": "./dist/package.json"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "test:types": "pnpm tsc --p ./tsconfig.test.json --noEmit",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/TypeGPU.git"
  },
  "keywords": [
    "webgpu",
    "wgpu",
    "wgsl",
    "typescript",
    "compute",
    "shader",
    "shaders",
    "gpgpu"
  ],
  "bugs": {
    "url": "https://github.com/software-mansion/TypeGPU/issues"
  },
  "homepage": "https://typegpu.com",
  "devDependencies": {
    "@ark/attest": "^0.46.0",
    "@typegpu/tgpu-dev-cli": "workspace:*",
    "@webgpu/types": "catalog:types",
    "arktype": "catalog:",
    "jiti": "catalog:build",
    "tsdown": "catalog:build",
    "typescript": "catalog:types",
    "unplugin-typegpu": "workspace:*",
    "wesl": "0.6.7",
    "wgpu-matrix": "catalog:example"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "dependencies": {
    "tinyest": "workspace:~0.1.1",
    "typed-binary": "^4.3.1"
  }
}
