{
  "name": "rollup-plugin-typegpu",
  "version": "0.0.1",
  "description": "A rollup plugin for TypeGPU, transpiling JavaScript functions into WGSL at build-time.",
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "typegpu",
    "gpgpu",
    "webgpu",
    "wgpu",
    "wgsl",
    "typescript",
    "shaders"
  ],
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.ts",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/TypeGPU.git"
  },
  "bugs": {
    "url": "https://github.com/software-mansion/TypeGPU/issues"
  },
  "homepage": "https://typegpu.com",
  "scripts": {
    "build": "tsup",
    "test:types": "pnpm tsc --p ./tsconfig.json --noEmit",
    "prepublishOnly": "tgpu-dev-cli prepack"
  },
  "dependencies": {
    "unplugin-typegpu": "workspace:^0.1.0-alpha.4"
  },
  "devDependencies": {
    "@typegpu/tgpu-dev-cli": "workspace:*",
    "rollup": "~4.22.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
